{
    cluster=arnold;
    pool="yt-integration-tests";
    pool_trees=[
        "physical";
    ];
    cypress_root="//home/yt-integration-tests";
    operation_spec={
        scheduling_tag_filter="!cpu_vendor_amd";
        job_cpu_monitor={
            enable_cpu_reclaim=%false;
        };
    };
    task_spec={
        monitoring={
            enable=%true;
            sensor_names=[
                "cpu/burst";
                "cpu/user";
                "cpu/system";
                "cpu/wait";
                "cpu/throttled";
                "cpu/cfs_throttled";
                "current_memory/rss";
                "current_memory/mapped_file";
                "current_memory/major_page_faults";
                "tmpfs_size";
                "disk/usage";
                "disk/limit";
            ];
        };
        use_porto_memory_tracking=%true;
    };
    run_as_root=%true;
    enable_network=%false;
    blob_ttl=302400000000000;
    output_ttl=604800000000000;
}
