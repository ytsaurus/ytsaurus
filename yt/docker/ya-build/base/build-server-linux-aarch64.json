{
    "build": {
        "build::server_binaries": {
            "targets": [
                "yt/yt/server/all",
            ],
            "build_type": "profile",
            "thinlto": true,
            "target-platforms": [
                "default-linux-aarch64",
            ],
            "flags": [
                {
                    "name": "NO_STRIP",
                    "value": "yes",
                },
            ],
        },
        "build::server_tools": {
            "targets": [
                "yt/python/yt/environment/bin/init_operations_archive",
                "yt/python/yt/environment/bin/init_queue_agent_state",
            ],
            "build_type": "release",
            "target-platforms": [
                "default-linux-aarch64",
            ],
        },
    },
}
